{
  "name": "disassembly",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_line"
      }
    },
    "_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "hexadecimal"
          },
          "named": true,
          "value": "address"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "code_location"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "machine_code_bytes"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "_instruction"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "_instruction"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_new_line"
        }
      ]
    },
    "code_location": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "code_identifier"
          },
          "named": true,
          "value": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "+"
                },
                {
                  "type": "SYMBOL",
                  "name": "integer"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "machine_code_bytes": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "byte"
      }
    },
    "hexadecimal": {
      "type": "PATTERN",
      "value": "0[xh][0-9a-fA-F]+"
    },
    "integer": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "byte": {
      "type": "PATTERN",
      "value": "[0-9a-fA-F]{2}"
    },
    "_instruction": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bad_instruction"
        },
        {
          "type": "SYMBOL",
          "name": "instruction"
        }
      ]
    },
    "bad_instruction": {
      "type": "STRING",
      "value": "(bad)"
    },
    "_new_line": {
      "type": "STRING",
      "value": "\n"
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "SYMBOL",
              "name": "hexadecimal"
            },
            {
              "type": "PATTERN",
              "value": "[^\\n]*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "^"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_word"
                    }
                  ]
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "<-"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "_word"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    "_word": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9\\.]+"
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s\\f\\uFEFF\\u2060\\u200B]|\\\\\\r?\\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "code_identifier"
    },
    {
      "type": "SYMBOL",
      "name": "instruction"
    },
    {
      "type": "SYMBOL",
      "name": "_error_sentinel"
    }
  ],
  "inline": [],
  "supertypes": []
}

